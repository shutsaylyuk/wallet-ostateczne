{% extends 'base.html.twig' %}
{% block title %}{{ 'email.change.title'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'email.change.heading'|trans }}</h1>
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    <button class="btn btn-primary">{{ 'email.change.submit'|trans }}</button>
    {{ form_end(form) }}
{% endblock %}