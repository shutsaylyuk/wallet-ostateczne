{% extends 'base.html.twig' %}
{% block title %}{{ 'password.change.title'|trans }}{% endblock %}

{% block body %}
    <h1>{{ 'password.change.heading'|trans }}</h1>
    {{ form_start(form) }}
    {{ form_errors(form.plainPassword) }}
    {{ form_row(form.plainPassword) }}
    <button class="btn btn-primary">{{ 'password.change.submit'|trans }}</button>
    {{ form_end(form) }}
{% endblock %}